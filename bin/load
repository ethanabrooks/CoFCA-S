#!/usr/bin/env zsh

config=$(${0:a:h}/search $1 config.yaml)
load=$(${0:a:h}/search $1 checkpoint.pt)
failure_buffer_load_path=$(${0:a:h}/search $1 failure_buffer.pkl)
cmd="python starcraft.py --config $config --load $load --failure_buffer_load_path $failure_buffer_load_path --no_wandb ${@:2}"
echo "$cmd\n"
eval "$cmd"
