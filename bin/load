#!/usr/bin/env zsh

config=$(${0:a:h}/search ${@[1, 2]} params.json)
load=$(${0:a:h}/search ${@[1, 2]} checkpoint.pt)
failure_buffer_load_path=$(${0:a:h}/search ${@[1, 2]} failure_buffer.pkl)
cmd="python starcraft.py --config $config --load $load --failure-buffer-load-path $failure_buffer_load_path ${@:3}"
echo "$cmd\n"
eval "$cmd"
